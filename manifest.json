{
    "manifest_version": 3,
    "name": "My Basic Extension",
    "version": "1.0",
    "description": "A simple Chrome extension",
    "permissions": ["activeTab", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon-48.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }